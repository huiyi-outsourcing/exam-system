<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="ExamSystem.models"
                   assembly="ExamSystem">

    <!-- Mappings for class 'Customer' -->
    <class name="Occupation" table="occupation">

        <!-- Identity mapping -->
        <id name="ID">
            <column name="id" />
            <generator class="native" />
        </id>
        
        <!-- enum type -->
        <property name="Status" column="status" sql-type="varchar(10)" not-null="true" />
    </class>

</hibernate-mapping>